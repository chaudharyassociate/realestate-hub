# Environment Configuration for CID Copilot Development

# Application Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# AI Model Endpoints
GPT_OSS_ENDPOINT=http://localhost:8001
LLAMA3_ENDPOINT=http://localhost:8002
QWEN3_ENDPOINT=http://localhost:8003

# Cybersecurity Tools Endpoints
SNIPER_ENDPOINT=http://localhost:8080
RECONNESS_ENDPOINT=http://localhost:8081
NODEGOAT_ENDPOINT=http://localhost:4000

# API Configuration
API_TIMEOUT=30000
MAX_RETRIES=3

# Security
JWT_SECRET=your-jwt-secret-here
CORS_ORIGIN=http://localhost:3000

# Database (if needed)
DATABASE_URL=postgresql://user:pass@localhost:5432/cidcopilot

# OpenAI API (if using real OpenAI)
# OPENAI_API_KEY=your-openai-key-here

# Feature Flags
ENABLE_PENTESTING=true
ENABLE_AI_ANALYSIS=true
ENABLE_DOCKER_INTEGRATION=true